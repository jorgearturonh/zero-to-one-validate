    "prettier": "^1.19.1"
  },
  "dependencies": {
    "@langchain/core": "^0.3.22",
    "axios": "^1.3.4",
    "cheerio": "^1.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsdom": "^25.0.1",
    "langchain": "^0.3.6",
    "module-alias": "^2.2.3",
